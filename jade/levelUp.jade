#levelup(ng-hide='!showLevel')
  .row
    .col.col25
      img#trophy(src='img/trophy_200x200.png', alt='trophy')
    .col.col75
      h1.subtitle Level {{game.currentLevel + 1}} Complete!
      button.button.icon-right.ion-play.button-large.button-balanced.button-block(ng-click='nextLevel(10, 1, game.score)') Continue &nbsp;
